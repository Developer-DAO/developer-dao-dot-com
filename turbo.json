{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/main",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": []
    },
    "test": {
      "outputs": []
    },
    "lint-staged": {
      "outputs": []
    },
    "dev": {
      "cache": false,
      "outputs": []
    },
    "deploy": {
      "dependsOn": [
        "cms#build",
        "cms#deploy",
        "frontend#build",
        "frontend#deploy"
      ],
      "outputs": []
    },
    "start": {
      "dependsOn": [
        "build"
      ],
      "outputs": [
        "build/**",
        ".next/**"
      ]
    },
    "cms#deploy": {
      "dependsOn": [
        "cms#build"
      ],
      "outputs": []
    },
    "cms#build": {
      "outputs": []
    },
    "frontend#deploy": {
      "dependsOn": [
        "frontend#build"
      ],
      "outputs": []
    },
    "frontend#build": {
      "dependsOn": [
        "cms#build"
      ],
      "outputs": []
    }
  }
}